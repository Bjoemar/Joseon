<div class="loginForm">
	<button class="close_log_btn">X</button>
	<h2>로그인</h2>
	<div class="login_inputs" style="text-align: left;">
		<form action="/user/login" method="POST" id="log_inner_form">
			<small style="color: red;"></small>
			<input type="text" name="log_user_id" placeholder="아이디">
			<small style="color: red;"></small>
			<input type="password" name="log_password" placeholder="비밀번호">
			<button id="login_credentials" type="button">로그인</button>
			<button class="regOpen"  type="button">회원가입</button>
		</form>
	</div>
</div>



<div class="regForm">
	<form action="/user/register" method="POST" id="reg_inner_form">
		<button class="close_reg_btn" type="button">X</button>
		<h2>회원가입</h2>
		<div class="reg_inputs">
			<small></small>
			<input type="text" name="reg_user_id" placeholder="아이디">
			<small></small>
			<input type="password" name="reg_password" placeholder="비밀번호">
			<small></small>
			<input type="password" name="reg_password2" placeholder="비밀번호 확인">
			<small></small>
			<input type="text" name="reg_name" placeholder="이름">
			<small style="float: left; width: 100%;"></small>
			<input id="cp_input" type="text" name="reg_cellphone"   placeholder="예) +821044445555">
			<button  id="register_verification"type="button">코드 받기</button>
			<small style="float: left; width: 100%;"></small>
			<input type="text" name="sms_codes" placeholder="코드 4자리" style="float: left; width: 70%;">
			<button type="button" id="verifycodes" style="float: left; width: 30%; margin: 0; font-size: 12px;">코드 확인</button>

			<button type="button" id="register_credentials">확인</button>
			
		</div>
	</form>
</div>